            <TableColumn fx:id="tableColumnPhase2" prefWidth="75.0" text="Фаза 2 зам." />
            <TableColumn fx:id="tableColumnMode" prefWidth="75.0" text="Режим фазы" />
        </columns>
         <columnResizePolicy>
